     RowBox[{"{", 
      RowBox[{"n", ",", "\[Rho]"}], "}"}], ",", 
     RowBox[{
      RowBox[{"Swap", "[", 
       RowBox[{"Sqrt", "[", "n", "]"}], "]"}], ".", "\[Rho]", ".", 
