  RowBox[{
   RowBox[{"UpperShiftMatrix", "[", "dim_", "]"}], ":=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"i", "<", "j"}], "&&", 
        RowBox[{
         RowBox[{"j", "-", "i"}], "==", "1"}]}], ",", "1", ",", "0"}], "]"}], 
     ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "dim"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", "dim"}], "}"}]}], "]"}]}], ";"}]], "Input"],

