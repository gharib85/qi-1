Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Swap2", "[", 
    RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
   RowBox[{"Sum", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Unvec", "[", 
       RowBox[{
        RowBox[{"UnitVector", "[", 
         RowBox[{
          RowBox[{"m", " ", "n"}], ",", 
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"i", "-", "1"}], ")"}], " ", "n"}], "+", " ", "j"}]}], 
         "]"}], ",", "n"}], "]"}], "\[CircleTimes]", 
      RowBox[{
       RowBox[{"Unvec", "[", 
        RowBox[{
         RowBox[{"UnitVector", "[", 
          RowBox[{
           RowBox[{"m", " ", "n"}], ",", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"i", "-", "1"}], ")"}], " ", "n"}], "+", " ", "j"}]}], 
          "]"}], ",", "n"}], "]"}], "\[Transpose]"}]}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "m"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"j", ",", "1", ",", "n"}], "}"}]}], "]"}]}], ";"}]], "Input"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"Swap2", "::", "usage"}], " ", "=", " ", 
   "\"\<Swap2[m,n] returns swap operations on m\[Cross]n-dimensional system, \
which can be used to represent a resuffling operation. See: Horn, Johnson, \
Topics in Matrix Analysis, Chapter 4.3. See also: Resuffle.\>\""}], " ", 
  ";"}]], "Input",
 CellChangeTimes->{{3.469524324205984*^9, 3.469524430204637*^9}}]
}, Open  ]],

