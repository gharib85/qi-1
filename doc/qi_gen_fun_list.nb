Cell["Find all names and generate LaTeX", "Subsubtitle",
 CellChangeTimes->{{3.467457550309866*^9, 3.467457591255776*^9}}],

Cell[BoxData[{
 RowBox[{
  RowBox[{"ExportString", "[", 
   RowBox[{"\"\<\>\"", ",", "\"\<TeX\>\""}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"f", "=", 
   RowBox[{
   "OpenWrite", "[", 
    "\"\</home/jam/zksi-repo/qi/doc/qi_functions_list.tex\>\"", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"print", "=", "False"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"latexList", "=", "\"\<\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"For", "[", 
   RowBox[{
    RowBox[{"i", "=", "1"}], ",", 
    RowBox[{"i", "\[LessEqual]", 
     RowBox[{"Length", "[", "txt", "]"}]}], ",", 
    RowBox[{"i", "++"}], ",", " ", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"StringMatchQ", "[", 
        RowBox[{
         RowBox[{"ToString", "[", 
          RowBox[{"txt", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "]"}], ",", 
         "\"\<(*Help messages*)\>\""}], "]"}], ",", 
       RowBox[{
        RowBox[{"print", "=", "True"}], ";"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"StringMatchQ", "[", 
        RowBox[{
         RowBox[{"ToString", "[", 
          RowBox[{"txt", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "]"}], ",", 
         "\"\<(*Private definitions*)\>\""}], "]"}], ",", 
       RowBox[{
        RowBox[{"print", "=", "False"}], ";"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"StringMatchQ", "[", 
         RowBox[{
          RowBox[{"ToString", "[", 
           RowBox[{"txt", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "]"}], ",", 
          RowBox[{"___", "~~", "\"\<::Subsection::\>\"", "~~", "___"}]}], 
         "]"}], "&&", "print"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"secLine", "=", 
         RowBox[{"txt", "[", 
          RowBox[{"[", 
           RowBox[{"i", "+", "1"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"secLine", "=", 
         RowBox[{"StringReplace", "[", 
          RowBox[{"secLine", ",", 
           RowBox[{
           "\"\<(*\>\"", "\[Rule]", " ", "\"\<\\\\subsection{\>\""}]}], 
          "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"secLine", "=", 
         RowBox[{"StringReplace", "[", 
          RowBox[{"secLine", ",", 
           RowBox[{"\"\<*)\>\"", "\[Rule]", " ", "\"\<}\>\""}]}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"latexList", " ", "=", " ", 
         RowBox[{
         "latexList", " ", "<>", " ", "secLine", "  ", "<>", " ", 
          "\"\<\\n\\n\>\""}]}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"If", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"StringMatchQ", "[", 
         RowBox[{
          RowBox[{"ToString", "[", 
           RowBox[{"txt", "[", 
            RowBox[{"[", "i", "]"}], "]"}], "]"}], ",", 
          RowBox[{"___", "~~", "\"\<::usage\>\"", "~~", "___"}]}], "]"}], "&&",
         "print"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"line", "=", 
         RowBox[{"txt", "[", 
          RowBox[{"[", "i", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"nameLength", "=", 
         RowBox[{
          RowBox[{"StringPosition", "[", 
           RowBox[{"line", ",", "\"\<::usage\>\""}], "]"}], "[", 
          RowBox[{"[", 
           RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"name", "=", 
         RowBox[{"StringDrop", "[", 
          RowBox[{"line", ",", 
           RowBox[{"{", 
            RowBox[{"nameLength", ",", 
             RowBox[{"StringLength", "[", "line", "]"}]}], "}"}]}], "]"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"usage", "=", 
         RowBox[{"ToString", "[", 
          RowBox[{"TeXForm", "[", 
           RowBox[{"DisplayForm", "[", 
            RowBox[{"ToExpression", "[", 
             RowBox[{"Evaluate", "[", 
              RowBox[{"name", "<>", "\"\<::usage\>\""}], "]"}], "]"}], "]"}], 
           "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"name", "=", 
         RowBox[{"StringReplace", "[", 
          RowBox[{
           RowBox[{"ToString", "[", 
            RowBox[{"FullForm", "[", "name", "]"}], "]"}], ",", 
           RowBox[{"\"\<\\\\\\\\\>\"", "\[Rule]", "\"\<\\\\\>\""}]}], "]"}]}],
         ";", "\[IndentingNewLine]", 
        RowBox[{"name", "=", 
         RowBox[{"ToString", "[", 
          RowBox[{"TeXForm", "[", 
           RowBox[{"ToExpression", "[", "name", "]"}], "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"res", "=", 
         RowBox[{"QI`Private`qiFormatUsageMsg", "[", 
          RowBox[{"name", ",", "usage"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"latexList", "=", 
         RowBox[{
         "latexList", "<>", "\"\<\\\\noindent\>\"", "<>", "res", "<>", 
          "\"\<\\\\\\\\\\n\\n\>\""}]}], " ", ";"}]}], "\[IndentingNewLine]", 
      "]"}]}]}], "\[IndentingNewLine]", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"WriteString", "[", 
   RowBox[{"f", ",", "latexList"}], "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"WriteString", "[", 
   RowBox[{"f", ",", "\"\<\\n\\n\>\""}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Close", "[", "f", "]"}], ";"}]}], "Input",
